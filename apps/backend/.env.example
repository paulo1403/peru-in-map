# Environment variables for QueHacer.pe Backend

# Node environment
NODE_ENV=development

# Server port
PORT=3000

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/quehacer_pe"

# CORS origin (frontend URL)
# En desarrollo, cualquier puerto localhost será permitido automáticamente
# En producción, especifica el dominio exacto
CORS_ORIGIN="http://localhost:4321"

# Logging level
LOG_LEVEL=info